{
  "schemaVersion": 1,
  "id": "mccolosseum",
  "version": "${version}",

  "name": "Minecraft Colosseum",
  "description": "This is a Minecraft 1.19 mod that adds in Colosseums structures where you can test your strength against bosses in front of a crowd for a reward.",
  "authors": [
    "The Changer"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GPL-3.0 License",
  "icon": "assets/mccolosseum/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.the_changer.mccolosseum.mccolosseum"
    ],
    "client": [
      "com.the_changer.mccolosseum.TutorialClientMod"
    ]
  },
  "mixins": [
    "mccolosseum.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "geckolib3": ">=3.1.6"
  },
  "suggests": {
    "another-mod": "*"
  }
}
