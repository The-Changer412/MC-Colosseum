{
  "schemaVersion": 1,
  "id": "mccolosseum",
  "version": "${version}",

  "name": "Minecraft Colosseum",
  "description": "This is a fabric 1.19 mod generate colosseum for you to fight three bosses in front of a crowd for rewards.",
  "authors": [
    "The Changer"
  ],
  "contact": {
    "homepage": "https://github.com/The-Changer412/MC-Colosseum",
    "sources": "https://github.com/The-Changer412/MC-Colosseum"
  },

  "license": "GPL-3.0 License",
  "icon": "assets/mccolosseum/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.the_changer.mccolosseum.mccolosseum"
    ],
    "client": [
      "com.the_changer.mccolosseum.mccolosseumClient"
    ]
  },
  "mixins": [
    "mccolosseum.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "geckolib3": ">=3.1.6"
  },
  "suggests": {
    "another-mod": "*"
